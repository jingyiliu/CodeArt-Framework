<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="codeArt.EasyMQ" type="CodeArt.EasyMQ.EasyMQConfiguration, CodeArt.EasyMQ"/>
    <section name="codeArt.RabbitMQ" type="CodeArt.RabbitMQ.RabbitMQConfiguration, CodeArt.RabbitMQ"/>
  </configSections>

  <codeArt.EasyMQ>
    <event>
      <subscriber group="1"/>
    </event>
  </codeArt.EasyMQ>


  <codeArt.RabbitMQ>
    <policyGroup>
      <!-- name:策略名称 -->
      <!-- server:rabbitMQ的宿主地址，可以使用标准格式host：port（例如host = myhost.com：15672）。如果省略端口号，则使用默认的AMQP端口（15672）。 -->
      <!-- virtualHost:虚拟主机名称 -->
      <policy name="event" host="127.0.0.1" virtualHost="test" userName="yj1118" password="Q!1" prefetchCount="50"/>
    </policyGroup>
  </codeArt.RabbitMQ>

<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7"/></startup></configuration>
