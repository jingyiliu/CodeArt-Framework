      <!-- virtualHost:虚拟主机名称 -->
      <policy name="rpc" host="127.0.0.1" virtualHost="test" userName="yj1118" password="Q!1" prefetchCount="50"/>
    </policyGroup>
  </codeArt.RabbitMQ>
