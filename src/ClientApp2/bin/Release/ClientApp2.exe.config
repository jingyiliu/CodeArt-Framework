<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="codeArt.RabbitMQ" type="CodeArt.RabbitMQ.RabbitMQConfiguration, CodeArt.RabbitMQ"/>
  </configSections>

  <codeArt.RabbitMQ>
    <policyGroup>
      <!-- name:策略名称 -->
